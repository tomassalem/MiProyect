<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<%- include('./partials/head') %>

<body>
	<!-- Header -->
	<% if (log) { %>
		<%- include('./partials/headerLogueado') %>
	 
	<% } else{ %>
		<%- include('./partials/headerNoLogueado') %>

	<% } %>

	<!-- /Header -->	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Resultados de búsqueda</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% for(let i=0; i<productosCompu.length; i++) {%>    
				<div class="col-12 col-sm-6 col-lg-3">
					<section class="product-box">
						<a href="/product/detail/<%=productosCompu[i].id%>">
							<article class="p-3 text-center"> 
								<h2 class="hipervinculo"> <%= productosCompu[i].titulo %></h2>
								<figure class="product-box_image">
									<img src="/images/products/<%= productosCompu[i].image %>" alt="">
								</figure>
								<article class="product-box_data">
									<p>Descripción: <%= productosCompu[i].description %></p>
								</article>
								
							
						    </article>
						</a>
					</section>
				</div>
		    <% } %>
		</div>
	</div>

	<!-- footer -->
	<%- include('./partials/footer') %> 
	<!-- /footer -->
</body>

</html>