<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<%- include('./partials/head') %>

<body>
	<!-- Header -->
<%- include('./partials/headerNoLogueado') %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Resultados de búsqueda</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% for(let i=0; i<8; i++) {%>    
				<div class="col-12 col-sm-6 col-lg-3">
					<section class="product-box">
						<a href="/product/detail/<%=productosCompu.lista[i].id%>">
							<article class="p-3 text-center"> 
								<h2 class="hipervinculo"> <%= productosCompu.lista[i].nombre %> </h2>
								<h4><%= productosCompu.lista[i].precio %></h4>
								<figure class="product-box_image">
									<img src="<%= productosCompu.lista[i].foto %>" alt="">
								</figure>
								<article class="product-box_data">
									<p>Descripción: <%= productosCompu.lista[i].descripcion %></p>
								</article>
								
							
						    </article>
						</a>
					</section>
				</div>
		    <% } %>


	<!-- footer -->
	<%- include('./partials/footer') %> 
	<!-- /footer -->
</body>

</html>